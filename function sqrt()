# cAlculAtrice-scientifique
un outil scientifique qui permet de r√©Aliser un ensemble d'operAtions(soustrAction,Addition,multiplicAtion,division)
function sqrt()
{
     
     
    sqrt=$($1)
    echo "square est = $sqrt"
     
    return 1;
}
 
for ((i=1;i<=$#;i++)) do
{
     
     
    if   test ${!i} = "cos()"
    then
            cos($1) 
    elif  test ${!i} = "sin()"
    then
            sin($1)
    elif  test ${!i} = "sqrt"
    then
          sqrt($1)
    fi
}
done
